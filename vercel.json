{
  "version": 2,
  "builds": [
    {
      "src": "api/BardApi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/BardApi.py"
    },
    {
      "src": "/api(.*)",
      "dest": "api/BardApi.py"
    }
  ],
  "projectSettings": {
    "buildCommand": null,
    "devCommand": null,
    "framework": "nextjs",
    "commandForIgnoringBuildStep": "",
    "installCommand": null,
    "outputDirectory": null
}
}
